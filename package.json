{
  "name": "@cytube/mediaquery",
  "version": "0.0.23",
  "description": "Media provider metadata queries for CyTube",
  "main": "index.js",
  "scripts": {
    "dev": "babel -D --watch --source-maps --out-dir lib/ src/",
    "prepublish": "babel -D --source-maps --out-dir lib/ src/",
    "test": "mocha --recursive"
  },
  "author": "Calvin Montgomery",
  "license": "MIT",
  "dependencies": {
    "@calzoneman/jsli": "^2.0.1",
    "bluebird": "^2.9.12",
    "domutils": "^1.5.1",
    "htmlparser2": "^3.8.3",
    "prom-client": "^10.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-plugin-add-module-exports": "^1.0.2",
    "mocha": "^6.1.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "12"
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "bin": {
    "mediaquery": "bin/mediaquery.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyTube/mediaquery.git"
  },
  "keywords": [
    "CyTube"
  ],
  "bugs": {
    "url": "https://github.com/CyTube/mediaquery/issues"
  },
  "homepage": "https://github.com/CyTube/mediaquery#readme"
}
